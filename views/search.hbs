
<div>
 <div class="works">
    <div class="container">
        <div class="portfolio">
  {{#each articlesByCity}}
            <div class="portfolio__col">
                <div class="work">
                    <img class="work__image" src={{this.img}}>
                    <div class="work__content">
                        <div class="work__title">
                            {{this.carName}}
                            <!-- <time class="work__date" datetime="2019-11-22 19:00">2019</time> -->
                        </div>
                        <div class="work__cat">Трансмиссия: {{this.transmission}}</div>
                        <div class="work__cat">Год: {{this.productionYear}}</div>
                        <div class="work__cat">Город: {{this.city}}</div>
                        {{#if this.isActive}}
                        Свободна
            <button><a href="/service/{{ this._id }}">Забронировать</a></button>
                        {{else}}
                        Занята
                        {{/if}}
                    </div>
                </div>
            </div>
  {{/each}}
            </div>
            </div>
            </div>



